{
  "name": "cy_saucedemo",
  "version": "1.0.0",
  "description": "cypress framework for saucedemo",
  "main": "index.js",
  "scripts": {
    "cypress": "cypress open",
    "clean:reports": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\final_report",
    "pretest": "npm run clean:reports",
    "scripts": "cypress run",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/final_report/report.json",
    "generate-report": "marge cypress/reports/final_report/report.json -f report -o cypress/reports/final_report",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts"
  },
  "author": "alagherasimov",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "cypress": "^13.13.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-multi-reporters": "^1.6.4",
    "mocha": "^10.6.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "-": "^0.0.1",
    "save-dev": "^0.0.1-security"
  }
}
